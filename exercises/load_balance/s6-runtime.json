{
  "target": "bmv2",
  "p4info": "build/load_balance.p4.p4info.txt",
  "bmv2_json": "build/load_balance.json",
  "table_entries": [

    {
      "table": "MyIngress.ecmp_group",
      "match": {
        "hdr.ipv4.dstAddr": [
          "10.0.0.2",
          32
        ]
      },
      "action_name": "MyIngress.set_ecmp_select",
      "action_params": {
        "ecmp_base": 0,
        "ecmp_count": 4
      }
    },

    { "table": "MyIngress.ecmp_nhop", "match": { "meta.ecmp_select": 0 },
      "action_name": "MyIngress.set_nhop", "action_params": { "nhop_dmac": "08:00:00:00:02:00", "nhop_ipv4": "10.0.2.2", "port": 2 } },
    { "table": "MyIngress.ecmp_nhop", "match": { "meta.ecmp_select": 1 },
      "action_name": "MyIngress.set_nhop", "action_params": { "nhop_dmac": "08:00:00:00:03:00", "nhop_ipv4": "10.0.3.3", "port": 3 } },
    { "table": "MyIngress.ecmp_nhop", "match": { "meta.ecmp_select": 2 },
      "action_name": "MyIngress.set_nhop", "action_params": { "nhop_dmac": "08:00:00:00:04:00", "nhop_ipv4": "10.0.4.4", "port": 4 } },
    { "table": "MyIngress.ecmp_nhop", "match": { "meta.ecmp_select": 3 },
      "action_name": "MyIngress.set_nhop", "action_params": { "nhop_dmac": "08:00:00:00:05:00", "nhop_ipv4": "10.0.5.5", "port": 5 } },
    
    {
      "table": "MyIngress.ipv4_lpm", "match": { "hdr.ipv4.dstAddr": [ "10.0.2.2", 32 ] },
      "action_name": "MyIngress.ipv4_forward", "action_params": { "dstAddr": "08:00:00:00:02:22", "port": 1 }
    }
  ]
}